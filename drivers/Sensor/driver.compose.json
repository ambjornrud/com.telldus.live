{
  "id": "Sensor",
  "name": {
    "en": "Telldus Sensor"
  },
  "$extends": [
    "defaults"
  ],
  "class": "sensor",
  "capabilities": [
    "measure_temperature",
    "measure_humidity",
    "measure_power",
    "measure_voltage",
    "meter_power"
  ],
  "pair": [
    {
      "id": "list_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_my_devices"
      }
    },
    {
      "id": "add_my_devices",
      "template": "add_devices"
    }
  ]
}